#! /bin/sh

alacritty --class=Music -e 'mocp' &
alacritty -e ranger ~/.config/alacritty &
alacritty -e pipes.sh -p 5 &

bspc config window_gap 40 &

${EDITOR} ~/Documents/Startpages/style.css &
dunstify -t 5000 -u low -r '92813' "Volume: $(amixer sget Master | awk -F'[][]' '/dB/ { print $2 }')" &
sleep 3 && modified-scrot '%Y-%m-%d.png' -e 'mv $f ~/Pictures/rices'
